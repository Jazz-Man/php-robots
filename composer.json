{
  "name": "jazzman/php-robots",
  "description": "Generator robots.txt",
  "keywords": [
    "robots.txt"
  ],
  "license": "MIT",
  "config": {
    "sort-packages": true,
    "preferred-install": "dist",
    "optimize-autoloader": true,
    "classmap-authoritative": true
  },
  "authors": [
    {
      "name": "Vasyl Sokolyk",
      "email": "vsokolyk@gmail.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^8.1",
    "jazzman/singleton-trait": "^1.1"
  },
  "require-dev": {
    "jazzman/php-cs-fixer-rules": "^0.2.0",
    "phpunit/phpunit": "^11.2"
  },
  "autoload": {
    "psr-4": {
      "JazzMan\\Robots\\": "src/"
    }
  }
}
